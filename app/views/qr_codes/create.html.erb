<div class="container form_div">
  <div class="rows">
    <div class="col-md-10 col-md-offset-1">
      <table class="qr-code">
        <% @qr.modules.each_index do |x| %>
        <tr>
          <% @qr.modules.each_index do |y| %>
            <% if @qr.dark?(x,y) %>
              <td class="black"/>
            <% else %>
              <td class="white"/>
            <% end %>
          <% end %>
        </tr>
        <% end %>
      </table>
      <br />
      <br />
      <%= link_to "New QR Code", new_qr_code_path,class:"btn-success btn-raised btn" %>
    </div>
  </div>
</div>






